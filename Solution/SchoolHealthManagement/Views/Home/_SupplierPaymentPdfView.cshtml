@model IEnumerable<SchoolHealthManagement.Models.SupplierPaymentRequestDetail>

<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
<div class="container" style="border:2px black solid">
    
    <h3>Supplyer Payment Details</h3>
    <br/>
    <div class="row">
        <div class="col-xs-6">
            <p> Year</p>
            <p> Moth</p>
        </div>
        <div class="col-xs-6">
            <p> Provice</p>
            <p> Zone </p>
        </div>
    </div>
    <div class="row col-xs-10 " style="font-size: 16px"></div>
    <table class="table table-striped table-bordered">
        <thead>
        <tr>
            <th>
                Censors ID
            </th>
            <th>
                Supplier Name
            </th>
            <th>
                Bank Account No
            </th>
            <th>
                Bank
            </th>
            <th>
                Branch Name
            </th>
            <th>
                Amount
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.CensorsID
                </td>
                <td>
                    @item.Supplier.SupplierName
                </td>
                <td>
                    @item.Supplier.BankAccountNo
                </td>
                <td>
                    @item.Supplier.BankName
                </td>
                <td>
                    @item.BranchName
                </td>

                <td>
                    @item.Amount

                </td>
            </tr>
        }
        </tbody>
    </table>
</div>

<div class="row">
    <div class="col-xs-6">
        <p> Created By</p>
        <p> </p>
    </div>
    <div class="col-xs-6">
        <p> Approved By</p>
        <p> </p>
    </div>
</div>